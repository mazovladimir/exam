= form_for @user, method: :get, html: { class: "main-window" } do |f|
  p= @examquestion.ask
  - @examquestion.answers.each do |answer|
    p
    = f.radio_button :examinee_score, answer.give, required: true
    = f.label :score, answer.give

  p= button_to 'Предыдущий вопрос', user_prev_question_path(id: @examquestion.id), method: :get
  p= button_to 'Следующий вопрос', user_next_question_path(id: @examquestion.id), method: :get
